    "next": "15.5.9",
    "payload": "3.72.0",
    "react": "19.2.2",
    "react-day-picker": "8.10.1",
    "react-dom": "19.2.2",
    "react-hook-form": "^7.71.1",
    "resend": "^6.8.0",
    "sharp": "^0.34.5",
